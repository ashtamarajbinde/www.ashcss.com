//  ( 1 to 10 number print )
for (let num = 1 ; num < 10 ; num ++)
{
    console.log (num)
} 
//--------------------------------------------------------------------
// ( 25 table print )
for (let a = 1 ; a <= 10 ; a ++)
{
    console.log ( a * 25 )
}
//---------------------------------------------------------------
// (odd number print)
for ( let b = 1 ; b <= 30 ; b = b + 2)
{
    console.log ( b)
}
//-----------------------------------------------
// ( even number )
for ( let c = 1 ; c <= 15 ; c++)
{
    console.log ( c * 2 )
}
